#include <stdio.h>

int main()
{
    char test[50];
    gets(test);
    int n,j;
    n = strlen(test);
    
    for(int i =n/2 ;i<n;i++)
    {
        for(j=n/2;j<=i;j++)
        
        printf("%c",test[j]);
        
        if(i == n-1)
        {
            printf("\n");
            for(int q =0;q<n/2;q++)
            {
           for(j=n/2;j<n;j++)
        {
        printf("%c",test[j]);
        }
        for(int w =0;w<=q;w++)
        {
              printf("%c",test[w]);  
        }
    
        printf("\n");
            }
            
        }
        
        printf("\n");
       
    }
   
    return 0;
}
